   "source": [
    "def Entropy():\n",
    "    entropy = 0\n",
    "    lst = [10,20,30,40,50,60]\n",
    "    for i in lst:\n",
    "        P = 1/i\n",
    "        print(P)\n",
    "        if P > 0:\n",
    "            entropy += -P*np.log(P)\n",
    "    return entropy\n",
    "            "
   ]
